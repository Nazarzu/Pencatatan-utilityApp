{
  "cli": {
    "version": ">= 16.20.4",
    "appVersionSource": "remote"
  },
  "build": {
    "preview": {
      "android": {
        "buildType": "apk",
        "distribution": "internal"
      }
    },
    "internal-release": {
      "android": {
        "buildType": "apk",
        "distribution": "internal"
      }
    },
    "development": {
